@using EntityLayer.Concrete
@model List<SocialMedia>

<div class="container">
    <div class="my-3">
        <div class="h4">Walter Patterson</div>
        <p>Web Developer & Mobile Application Developer</p>
        <div class="social-nav">
            <nav role="navigation">
                <ul class="nav justify-content-center">
                    

                    @foreach (var item in Model)
                    {


                    <li class="nav-item"><a class="nav-link" href="@item.Url" title="@item.Name"><i class="@item.Icon"></i><span class="menu-title sr-only">@item.Name</span></a></li>
                    }
                    
                </ul>
            </nav>
        </div>
    </div>

</div>